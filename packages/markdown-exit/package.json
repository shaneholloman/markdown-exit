{
  "name": "markdown-exit",
  "type": "module",
  "version": "1.0.0-beta.7",
  "description": "A TypeScript rewrite of markdown-it with enhancements",
  "license": "MIT",
  "homepage": "https://github.com/serkodev/markdown-exit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serkodev/markdown-exit.git"
  },
  "bugs": {
    "url": "https://github.com/serkodev/markdown-exit/issues"
  },
  "keywords": [
    "markdown",
    "parser",
    "commonmark",
    "markdown-it",
    "markdown-it-plugin",
    "markdown-exit"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "entities": "catalog:integrations",
    "linkify-it": "catalog:integrations",
    "mdurl": "catalog:integrations",
    "punycode.js": "catalog:integrations",
    "uc.micro": "catalog:integrations"
  },
  "devDependencies": {
    "@markdown-exit/testgen": "workspace:*",
    "@types/linkify-it": "catalog:types",
    "@types/mdurl": "catalog:types"
  }
}
